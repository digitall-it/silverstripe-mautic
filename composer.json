{
    "name": "digitall-it/silverstripe-mautic",
    "description": "SilverStripe module to connect with Mautic",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4",
        "silverstripe/admin": "^1",
        "mautic/api-library": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "DigitallIt\\MauticSilverstripe\\": "src/",
            "DigitallIt\\MauticSilverstripe\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
